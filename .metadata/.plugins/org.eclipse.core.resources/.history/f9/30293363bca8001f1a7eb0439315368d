package com.nt.apexonLogicalProgram;


class Node{
	
	int data;
	Node left, right;
	
	public Node(int v) {
		
		data = v;
		left = null;
		right = null;
	}
}
public class InorderBinaryTree {

	Node root;
	
	public static void main(String[] args) {
		
		InorderBinaryTree bt = new InorderBinaryTree();
		
		bt.root = new Node(1);
		bt.root.left = new Node(2);
		bt.root.right = new Node(3);
	}
}
