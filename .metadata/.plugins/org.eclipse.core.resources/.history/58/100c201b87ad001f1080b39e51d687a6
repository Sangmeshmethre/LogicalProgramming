package com.nt.SiimplyCodingStringWord;

public class CountEndingWithD {

	public static void main(String[] args) {
		
		String str = "A Red Seed";
		str =str+" ";
		String word = "";
		int count = 0;
		
		for(int i=0; i<str.length(); i++) {
			
			char ch = str.charAt(i);
			
			if(ch !=' ') {
				word = word+ch;
			}
		}
	}
}
