package com.nt.ApexonLogical;

public class LargestWordInArray {

	public static void main(String[] args) {
		
		String[] dict = {"CODGE", "ODG", "LODGES", "SODG", "dodge","mODJ", "LODGESSSS"};
		String toString = "ODG";
		
		String word = "", lword = "";
		int maxLength = 0;
		
		for(int i=0; i<dict.length; i++) {
			
			String str = dict[i];
			str = str+" ";
		}
	}
}
